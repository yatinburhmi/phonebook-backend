{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
      "request": "launch",
      "name": "Debug API",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect"],
      "args": ["-r", "ts-node/register", "src/index.ts"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
      }
    ]
  }